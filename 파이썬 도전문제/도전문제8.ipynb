    "def getnums(num):\n",
    "    n=0\n",
    "    l=[1]\n",
    "    for i in range(num-1):\n",
    "            l.append(l[-1]+3**n)\n",
    "            n+=1\n",
    "    return l\n",
    "\n",
    "print(getnums(15))"
